{
    "name": "newshour/wp-core-theme-components",
    "type": "library",
    "license": "MIT",
    "description": "NewsHour Core Wordpress theme components.",
    "keywords": [],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.4",
        "doctrine/annotations": "^1.13",
        "nesbot/carbon": "^2.54",
        "symfony/asset": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/notifier": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/slack-notifier": "^5.4",
        "symfony/yaml": "^5.4",
        "timber/timber": "^1.19"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.0.2",
        "roave/security-advisories": "dev-master",
        "roots/wordpress": "^5.8"
    },
    "scripts": {
        "post-autoload-dump": [
            "NewsHour\\WPCoreThemeComponents\\Containers\\ContainerFactory::dumpAutoload"
        ],
        "tests": [
            "vendor/bin/phpunit --bootstrap vendor/autoload.php ./src/Tests",
            "phpcs --standard=psr12 ./src/"
        ]
    },
    "autoload": {
        "psr-4": {
            "NewsHour\\WPCoreThemeComponents\\": "src"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
